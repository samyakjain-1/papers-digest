{
  "builds": [
    {
      "src": ".modelence/build/app.mjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": ".modelence/build/app.mjs"
    },
    {
      "source": "/(.*)",
      "destination": ".modelence/build/client/index.html"
    }
  ]
}
