{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "includeFiles": [".modelence/build/**", "src/client/**"] }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": ".modelence/build/app.mjs"
    },
    {
      "src": "/(.*)",
      "dest": ".modelence/build/client/$1"
    }
  ]
}
